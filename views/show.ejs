<%- include("./partials/header") %>
	<div class="container">
		<div class="row" >
			<div style="width: 70%; margin: 20px auto; border-radius: 20px; padding: 20px; border: 1px solid #888888">
				<% for(var i=0; i<touristPlaces.length; i++){ %>
					<h3 class="display-4"><%= touristPlaces[i].title %></h3>
					<hr>
					<img src="<%= touristPlaces[i].image %>" style="width: 100%; height: 400px; background-size: cover; background-position: 50% 50%;">
					<p class="mt-3"><%= touristPlaces[i].description %></p>
					<hr style="border-top:2px solid black;">
				<% } %>
			</div>
	    </div>	
	</div>
<%- include("./partials/footer") %>